            "hostName": "localhost"
        },
        {
            "type": "java",
            "name": "Attach Maven Test<%service%>",
            "request": "attach",
            "port": "2%SERVICE_PORT%",
            "projectName": "%service%",
            "hostName": "localhost"
        }
    ]
}
