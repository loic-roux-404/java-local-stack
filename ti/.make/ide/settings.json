{
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "debug.focusWindowOnBreak": false,
    "redhat.telemetry.enabled": false,
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.saveActions.organizeImports": true,
    "java.autobuild.enabled": true,
    "java.server.launchMode": "Standard",
    "java.trace.server": "off",
    "java.completion.favoriteStaticMembers": [
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*",
        "org.assertj.core.api.Assertions.*",
        "org.hamcrest.CoreMatchers.*",
        "org.apache.commons.collections.*",
        "org.springframework.security.test.web.servlet.request.SecurityMockMvcRequestPostProcessors.*",
        "tech.jhipster.lite.module.domain.JHipsterModule.*",
        "tech.jhipster.lite.module.domain.JHipsterModulesFixture.*",
        "tech.jhipster.lite.module.domain.replacement.ReplacementCondition.*"
    ],
    "boot-java.validation.java.reconcilers": false,
    "java.checkstyle.configuration": "/google_checks.xml",
    "java.configuration.maven.globalSettings": "%MVN_SETTINGS%",
    "java.configuration.maven.userSettings": "%MVN_SETTINGS%",
    "java.debug.settings.vmArgs": "-Xdebug -XX:TieredStopAtLevel=1 -Xnoagent -Djava.compiler=NONE -Dspring.main.allow-bean-definition-overriding=true -Dspring.main.lazy-initialization=true %VM_ARGS%",
    "java.debug.settings.jdwp.async": "on",
    "java.format.settings.profile": "GoogleStyle",
    "java.jdt.ls.vmargs": "-Xms256m -Xmx640m -XX:MaxMetaspaceSize=128m -XX:MaxGCPauseMillis=200 -Xquickstart -XX:+IdleTuningGcOnIdle -XX:AdaptiveSizePolicyWeight=90 -XX:+UseCompressedOops -XX:+OptimizeStringConcat -XX:GCTimeRatio=10 -XX:+UseStringDeduplication -XX:+EnableCRIUSupport -XX:-CRIUSecProvider",
    "spring-boot.ls.java.vmargs": [
        "-Xms256m",
        "-Xmx640m",
        "-XX:MaxMetaspaceSize=128m",
        "-XX:MaxGCPauseMillis=500",
        "-Xquickstart",
        "-XX:+IdleTuningGcOnIdle",
        "-XX:AdaptiveSizePolicyWeight=90",
        "-XX:+UseCompressedOops",
        "-XX:+OptimizeStringConcat",
        "-XX:GCTimeRatio=10",
        "-XX:+UseStringDeduplication",
        "-XX:+EnableCRIUSupport",
        "-XX:-CRIUSecProvider"
    ],
    "java.jdt.ls.java.home": "%JAVA_HOME_LS%",
    "spring-boot.ls.java.home": "%JAVA_HOME_LS%",
    "java.settings.url": "./.settings/org.eclipse.jdt.core.prefs",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-%JAVA_SHORT_VERSION%",
            "path": "%JAVA_HOME%"
        }
    ],
    "java.debug.settings.stepping.skipClasses": [
        "$JDK",
        "java.*",
        "javax.*",
        "jakarta.*",
        "sun.*",
        "net.*",
        "antlr.*",
        "io.*",
        "org.antlr.*",
        "org.aspectj.*",
        "org.bouncycastle.*",
        "com.sun.*",
        "com.oracle.*",
        "jdk.*",
        "org.apache.*",
        "com.microsoft.*",
        "com.mysql.*",
        "org.hibernate.*",
        "com.google.*",
        "org.springframework.data.*",
        "com.zaxxer.*",
        "org.jetbrains.*",
        "org.intellij.*",
        "ch.qos.*",
        "dev.akkinoc.*",
        "kotlin.*",
        "org.junit.*",
        "org.joda.*",
        "feign.*"
    ],
    "java.debug.settings.stepping.skipSynthetics": true,
    "java.debug.settings.stepping.skipStaticInitializers": true,
    "java.test.defaultConfig": "tests",
    "java.debug.settings.jdwp.limitOfVariablesPerJdwpRequest": 750,
    "java.test.config": {
        "name": "tests",
        "javaExec": "%JAVA_HOME%/bin/java",
        "vmArgs": [
            "%JSON_TEST_ARGS%"
        ]
    },
    "files.exclude": {
        "**/.venv": true
    },
    "search.exclude": {
        "**/.venv": true,
        "dist/": true,
        "**/dist": true,
        "target/": true,
        "**/target": true
    },
    "search.useIgnoreFiles": false,
    "sonarlint.disableTelemetry": true,
    "sonarlint.focusOnNewCode": true,
    "terminal.integrated.defaultProfile.linux": "bash"
}
